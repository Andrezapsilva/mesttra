//declaracao das variaveis
var area; 
var valorMetroQuadro;
var metragemFrente;
var metragemLateral;
var valorFinalTerreno;

//obtencao dos dados inseridos no teclado pelo usario e armazenamento nas variaveis
metragemFrente = prompt("Informe a metragem da frente do terreno em metros: ");
metragemLateral = prompt("Informe a metragem da lateral do terreno em metros: ");
valorMetroQuadro = prompt("Informe o valor do metro quadrado: ");

//calculo da area do terreno
area = (metragemFrente * metragemLateral)   
//calculo do valor final do terreno
valorFinalTerreno = (area * valorMetroQuadro)

//exibicao dos dados na tela do usuario
console.log("Area total do terreno de ",metragemFrente," mts de frente e ",metragemLateral," mts de lateral é ",area," mts de area.") 
console.log("O valor deste terreno é: R$ ",valorFinalTerreno)


